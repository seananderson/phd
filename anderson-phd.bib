  doi		= {10.1073/pnas.0701685104},
  journal	= {Proc. Natl. Acad. Sci. U.S.A.},
  number	= {16},
  pages		= {6720--6725},
  title		= {Projected impacts of climate change on salmon habitat
		  restoration},
  volume	= {104},
  year		= {2007},
  bdsk-file-1	= {}
		  ,
