@Article{	  anderson1982,
  abstract	= {A striking and consistent observation in ecology is that
		  variability in population abundance of a species and
		  average population density are related in both space and
		  time1,2. Taylor and his colleagues have shown that this
		  relationship between variance (V) and mean (M) conforms
		  well to a simple power law for a very wide range of animal
		  and plant species, with the logarithms of sample variance
		  and average sample density following a linear
		  relationship1−3 (V = {\^a}M where {\^a} and are
		  constants). They suggest that the slope of this line, ,
		  which generally falls within the range 1 to 2, is a
		  species-specific characteristic reflecting the balance
		  between the opposing behavioural tendencies to aggregate
		  within, and migrate from, centres of population density4.
		  Taylor and co-workers1−6 have stressed that migration
		  between patches is rarely a random process, on the evidence
		  of the observed 'density-dependent' relationships between
		  the degree of dispersion (the variance to mean ratio, V/M
		  and mean abundance per patch). We show here that the
		  relationship between variability and average abundance can
		  be a simple, and inevitable, consequence of chance
		  demographic events in the dynamics of population growth and
		  decline. The precise form of these relationships is
		  determined by the relative magnitude of the various rate
		  processes which govern the dynamics of population change
		  (the birth, death, immigration and emigration rates), and
		  by the degree of spatial and temporal heterogeneity. We do
		  not need to invoke any complex behavioural mechanisms to
		  explain observed patterns.},
  author	= {Anderson, R. M. and Gordon, D. M. and Crawley, M. J. and
		  Hassell, M. P.},
  date-added	= {2011-06-16 15:26:36 -0700},
  date-modified	= {2013-05-28 02:51:29 +0000},
  doi		= {10.1038/296245a0},
  journal	= {Nature},
  number	= {5854},
  pages		= {245-248},
  title		= {Variability in the abundance of animal and plant species},
  volume	= {296},
  year		= {1982},
  bdsk-file-1	= {}
		  ,
  bdsk-url-1	= {http://dx.doi.org/10.1038/296245a0}
}

@Article{	  taylor1982,
  abstract	= {(1) In an initial analysis, the inter- and intra-specific
		  relationships between spatial and temporal variances and
		  means are examined comparatively within and between
		  ninety-seven aphid, 263 month and eighty-four bird species
		  at up to 300 sites for about a decade throughout Great
		  Britain. (2) Despite the vast amount of data available,
		  from which more than half the species sampled had been
		  removed before analysis because they were numerically
		  inadequate, parameter bias remains in some species and
		  results in interspecific artifacts. (3) Population
		  parameters dependent on small samples with arithmetic means
		  below two and variances below four, are subject to
		  distortion affecting all measures of aggregation. The
		  artifact of small discrete samples is illustrated by
		  spurious patterns in the relationship between log mean and
		  log variance. (4) In contrast, taxonomic defects in the
		  data were discovered by inconsistencies in the analysis of
		  spatio-temporal pattern because of the large numbers of
		  species sampled simultaneously. (5) The power function is
		  here fitted as a functional linear regression in logs to
		  provide an economical working model for the interdependence
		  between variance and mean density. (6) In multispecies
		  samples, the power relating mean and variance approaches
		  two in temporal regressions for aphids and moths and in
		  spatial regressions for aphids, as expected from
		  conventional theory, but not for moths in space or birds in
		  either space or time. (7) For separate species, the powers
		  range from 0.8 to 2.8 in time and 1.3 to 3.8 in space; mean
		  values for aphids, moths and birds are 1.99/2.14, 1.55/2.24
		  and 1.30/1.79, respectively, in time/space. (8) There is no
		  interspecific correlation between the spatial and temporal
		  powers relating variance and mean density. (9) The
		  interspecific spatial power is probably independent of
		  differences in population density between species. The
		  temporal power increases with mean population density
		  between species and this accounts for about 30{\%} of the
		  interspecific variance. (10) There is further evidence that
		  environment and sample size affect the factor, a, rather
		  than the power, b, in both space and time. (11) The bird
		  data show this in the disrupting effect, on some species'
		  population structures, of the constraints imposed on
		  environmental pattern by man. (12) The interspecific
		  relationship between the factor, a, and the power, b, has
		  common elements in all taxa and both space and time, but
		  scales differ; the pattern is complex and not yet fully
		  understood. (13) Only by using samples from such large
		  homogeneous groups of species sampled simultaneously could
		  any confidence be placed in these results. In particular,
		  the data from the two groups of moths were much the most
		  consistent despite the defects of attraction sampling. (14)
		  The analysis suggests that some of the evidence presented
		  for dynamic theory is suspect because its species' base is
		  narrow and its replication inadequate.},
  author	= {Taylor, L. R. and Woiwod, I. P.},
  date-added	= {2011-06-10 13:54:09 -0700},
  date-modified	= {2014-02-11 00:20:08 +0000},
  journal	= {J. Anim. Ecol.},
  keywords	= {taylors-power-law},
  number	= {3},
  pages		= {879--906},
  title		= {Comparative Synoptic Dynamics. {I.} {Relationships}
		  Between Inter- and Intra-Specific Spatial and Temporal
		  Variance/Mean Population Parameters},
  volume	= {51},
  year		= {1982},
  bdsk-file-1	= {}
		  
}

